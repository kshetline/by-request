  "description": "Simple Node http client for use with promises, async/await.",
  "main": "dist/index.js",
  "typings": "dist/index",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "mocha --require ts-node/register src/**/*.spec.ts",
